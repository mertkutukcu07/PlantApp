{
  "expo": {
    "name": "PlantApp",
    "slug": "PlantApp",
    "version": "1.0.0",
    "web": {
      "favicon": "./src/assets/config/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/config/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mertkutukcu07.PlantApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/config/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mertkutukcu07.PlantApp"
    }
  }
}
