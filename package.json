    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "resolutions": {
    "@react-native/assets-registry": "0.74.83"
  },
  "main": "node_modules/expo/AppEntry.js",
